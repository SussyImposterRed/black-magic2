local url = "https://raw.githubusercontent.com/SussyImposterRed/black-magic2/main"

local function import(file)
    -- return task.spawn(function()
    local x, a = pcall(function()
        return loadstring(game:HttpGet(url .. file))()
    end)
    if not x then
        return warn('failed to import', file)
    end
    -- end)
end

getgenv().import = import

if not import then
    return
end

import('/config')
import('/library')
import('/load_window')
